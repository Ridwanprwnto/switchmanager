{
    "name": "topology-network-management-api",
    "version": "1.0.0",
    "description": "Backend API for Topology Network Management System",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "env:dev": "cp .env.development.local .env",
        "env:test": "cp .env.test.local .env",
        "env:prod": "cp .env.production.local .env",
        "test": "jest",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "health-check": "curl http://localhost:5000/api-snm/health"
    },
    "keywords": [
        "topology",
        "network",
        "management",
        "api",
        "express",
        "mongodb"
    ],
    "author": "Ridwan Purwanto",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.1",
        "helmet": "^7.0.0",
        "jspdf": "^2.5.1",
        "mongoose": "^7.5.0"
    },
    "devDependencies": {
        "eslint": "^8.46.0",
        "jest": "^29.6.2",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0",
        "supertest": "^6.3.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Ridwanprwnto/switchmanager"
    }
}
